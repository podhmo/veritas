{
    "sources.MockUser": {
        "typeRules": [
            "self.Age >= 18"
        ],
        "fieldRules": {
            "Name": [
                "self.Name != \"\""
            ],
            "Email": [
                "self.Email != \"\" && self.Email.matches('^[^\\\\s@]+@[^\\\\s@]+\\\\.[^\\\\s@]+$')"
            ],
            "ID": [
                "has(self.ID)"
            ]
        }
    },
    "sources.Profile": {
        "fieldRules": {
            "Platform": [
                "self != \"\""
            ],
            "Handle": [
                "self != \"\" && self.size() > 2"
            ]
        }
    }
}
